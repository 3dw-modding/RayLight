{
    "version": "0.2.0",
    "configurations": [
        {
            // Use IntelliSense to find out which attributes exist for C# debugging
            // Use hover for the description of the existing attributes
            // For further information visit https://github.com/dotnet/vscode-csharp/blob/main/debugger-launchjson.md
            "name": ".NET Core Launch (ARM64)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build (ARM64)",
            "cwd": "${workspaceFolder}/RayLight",
            "program": "/usr/local/share/dotnet/dotnet",
            "args": "${workspaceFolder}/RayLight/bin/Debug/net9.0/RayLight.dll",
            "targetArchitecture": "arm64"
        },
        {
            // Use IntelliSense to find out which attributes exist for C# debugging
            // Use hover for the description of the existing attributes
            // For further information visit https://github.com/dotnet/vscode-csharp/blob/main/debugger-launchjson.md
            "name": ".NET Core Launch (X64)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build (X64)",
            "cwd": "${workspaceFolder}/RayLight",
            "program": "/usr/local/share/dotnet/x64/dotnet",
            "args": "${workspaceFolder}/RayLight/bin/X64/Debug/net9.0/RayLight.dll",
            "targetArchitecture": "x86_64"
        }
    ]
}